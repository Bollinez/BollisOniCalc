{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "build/**/*.js",
      "coverage/**/*.js"
    ]
  },
  "rules": {
    "curly": [true, "ignore-same-line"],
    "object-literal-sort-keys": false
  },
  "jsRules": {
    "curly": [true, "ignore-same-line"],
    "object-literal-sort-keys": false
  }
}
